import java.util.Scanner;

class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter a number:");
        int n=sc.nextInt();
        System.out.println(prod(n));
    }
    static int prod(int n){
        if(n%10==n){
            return n;
        }
        return (n%10)*prod(n/10);
        
    }
}

o/p:
enter a number:
123
6
