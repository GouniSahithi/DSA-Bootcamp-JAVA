import java.util.*;
public class Main{
    public static void main(String[] args){
        int[] arr={20,10,9,-300,1};
        System.out.println(min(arr));
    }
    static int min(int[] arr){
        if(arr.length==0){
            return -1;
        }
        int min=arr[0];
        for(int i=1;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
                
            }
        }
        return min;
        
    }
}
